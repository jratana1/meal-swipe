<%= form_for @user, :url => url_for(:controller => 'sessions', :action => 'create')do |f|%>
    <p>
        <%= f.label :Username %>
        <%= f.text_field :name %>
    </p>
    <p>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </p>

      <%= f.submit "Sign In"%>
<% end %>